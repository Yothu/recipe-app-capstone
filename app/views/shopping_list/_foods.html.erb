

<table class="table">
  <thead>
    <tr>

      <th scope="col">Food</th>
      <th scope="col">Quantity</th>
      <th scope="col">Price</th>
    </tr>
  </thead>
  <tbody>
    <% @foods.each do |f|%>
    <tr>
      <td><%= f.name %></td>
      <td><%= f.quantity_sum %> <%= f.measurement_unit %></td>
      <td>$<%=f.price * f.quantity_sum%></td>

    </tr>
<%end%>
  </tbody>
</table>
